{
	"coc.preferences.enableFloatHighlight": true,
	"coc.preferences.extensionUpdateCheck": "daily",
	"coc.preferences.messageLevel": "error",
	"coc.preferences.formatOnSaveFiletypes": [
		"javascript",
		"css",
		"html",
		"json",
		"python",
		"vue"
	],
	"highlight.colors.enable": false,
	"highlight.colorNames.enable": true,
	"suggest.labelMaxLength": 40,
	"suggest.detailMaxLength": 40,
	"suggest.noselect": true,
	"suggest.enablePreselect": false,
	"suggest.completionItemKindLabels": {
		"keyword": "\uf1de",
		"variable": "\ue79b",
		"value": "\uf89f",
		"operator": "\u03a8",
		"function": "\u0192",
		"reference": "\ufa46",
		"constant": "\uf8fe",
		"method": "\uf09a",
		"struct": "\ufb44",
		"class": "\uf0e8",
		"interface": "\uf417",
		"text": "\ue612",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"module": "\uf40d",
		"color": "\ue22b",
		"property": "\ue624",
		"field": "",
		"unit": "\uf475",
		"event": "\ufacd",
		"file": "\uf15c",
		"folder": "\uf114",
		"snippet": "\ue60b",
		"typeParameter": "\uf728",
		"default": "\uf29c"
	},
	"html.suggest.html5": true,
	"html.format.enable": true,
	"html.validate.scripts": true,
	"html.validate.styles": true,
	"html.autoClosingTags": true,
	"html.format.indentInnerHtml": false,
	"snippets.ultisnips.enable": true,
	"snippets.ultisnips.directories": [
		"$HOME/.config/nvim/plugged/vim-snippets/snippets"
	],
	"snippets.snipmate.author": "ysx",
	"snippets.userSnippetsDirectory": "~/.config/nvim/my_snippets",
	"explorer.width": 38,
	"explorer.quitOnOpen": true,
	"explorer.icon.enableNerdfont": true,
	"diagnostic.enableSign": true,
	"diagnostic.displayByAle": false,
	"diagnostic.refreshOnInsertMode": false,
	"diagnostic.checkCurrentLine": true,
	"diagnostic.virtualTextPrefix": " ❯❯❯ ",
	"diagnostic.virtualText": true,
	"diagnostic.virtualTextLines": 3,
	"diagnostic.virtualTextCurrentLineOnly": false,
	"yank.highlight.enable": true,
	"list.source.files.defaultOptions": ["--auto-preview"],
	"list.source.outline.defaultOptions": ["--auto-preview"],
	"list.previewHighlightGroup": "Statement",
	"python.pythonPath": "python",
	"prettier.useTabs": true,
	"languageserver": {
		"golang": {
			"command": "~/go/bin/gopls",
			"rootPatterns": ["go.mod"],
			"disableWorkspaceFolders": true,
			"filetypes": ["go"]
		},
		"ccls": {
			"command": "ccls",
			"filetypes": ["c", "cpp", "cuda", "objc", "objcpp"],
			"rootPatterns": [".ccls-root", "compile_commands.json"],
			"initializationOptions": {
				"cache": {
					"directory": ".ccls-cache"
				},
				"client": {},
				"highlight": {
					"lsRanges": true
				}
			}
		}
	}
}
